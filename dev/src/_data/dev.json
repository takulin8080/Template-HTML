		"ejscode": "<%#\r\n=====================================================================================================\r\nfilename\r\n=====================================================================================================\r\n%>\r\n<%- include(ejspath + '_template/' + template, {modifier: 'before'}) %>\r\n<%# ---------------------------------------------------------------------------------------------- %>\r\n<%# ---------------------------------------------------------------------------------------------- %>\r\n<%- include(ejspath + '_template/' + template, {modifier: 'after'}) %>",
		"sasscode": "/* ============================================================================================== */\r\n/* scope/filename\r\n/* ============================================================================================== */\r\nbody[modifier] {}",
		"modEjscode": "<%#\r\n=====================================================================================================\r\nfilename\r\n=====================================================================================================\r\n%>\r\n<%\r\n\rif (typeof modifier === 'undefined') { var modifier = ''; };\r\nvar fileHierarchy;\r\n%>\r\n<%# ---------------------------------------------------------------------------------------------- %>\r\n<!-- classname -->\r\n<!-- /classname -->\r\n",
		"modSasscode": "/* ============================================================================================== */\r\n/* module/filename\r\n/* ============================================================================================== */\r\n/*\r\n---\r\nname: filename\r\ncategory: 4.module\r\n---\r\n*/\r\n.classname {}",
		"templateEjscode": "<%#\r\n=====================================================================================================\r\ntemplate/filename\r\n=====================================================================================================\r\n%>\r\n<%\r\nvar fileHierarchy;\r\n%>\r\n<%# ---------------------------------------------------------------------------------------------- %>",
		"layoutSasscode": "/* ============================================================================================== */\r\n/* layout/filename\r\n/* ============================================================================================== */\r\n/*\r\n---\r\nname: filename\r\ncategory: 3.layout\r\n---\r\n*/\r\nbody[data-modifier^='tempModifier'] {}"
	}
