<%#
=====================================================================================================
_data/json-ld/SiteNavigationElement
=====================================================================================================
%>
<%
var schemaKey = 'SiteNavigationElement';
if (typeof SiteNavigationElement === 'undefined') { var SiteNavigationElement = '' };
if(SiteNavigationElement) {
	var obj = SiteNavigationElement;
	var objArray = obj;
} else {
	var obj = data[schemaKey];
	var objArray = obj;
}
var ejspath = '../../';
%>
<%# ---------------------------------------------------------------------------------------------- %>
{
	"@context": "http://schema.org",
	"@type": "SiteNavigationElement",
	"name": [
		<% objArray.forEach(function(key, index) { %>
			<% if (!index) { %>
				"<%- data.schema.WebSite.name %>"
			<% } else { %>
				"<%- page[key].title %>"
			<% } %>
			<% if( index != objArray.length - 1 ){ %>
				,
			<% } %>
		<% }) %>
	],
	"url": [
		<% objArray.forEach(function(key, index) { %>
			"<%- data.schema.WebSite.url %><%- page[key].absUrl %>"
			<% if( index != objArray.length - 1 ){ %>
				,
			<% } %>
		<% }) %>
	]
}
