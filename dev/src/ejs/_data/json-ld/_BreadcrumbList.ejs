<%#
=====================================================================================================
_data/json-ld/_BreadcrumbList
=====================================================================================================
%><script type="application/ld+json">
	[
		{
			"@context": "http://schema.org/",
			"@type": "BreadcrumbList",
			"itemListElement": [
				<% parent.forEach(function(value, key) { %>
					{
						"@type": "ListItem",
						"position": <%- key + 1 %>,
						"item": {
						"@id": "<%- data.schema.WebSite.url %><%- page[value].absUrl %>",
						"name": "<%- page[value].title %>"
						}
					},
				<% }) %>
				{
					"@type": "ListItem",
					"position": <%- parent.length + 1 %>,
					"item": {
						"@id": "<%- data.schema.WebSite.url %><%- absUrl %>",
						"name": "<%- title %>"
					}
				}
			]
		}
	]
</script>
